<%- include("template/header", {WebTitle: "Pagina Index"} )%>
    
   <div class="container">

    <table class="table">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Action</th>
          </tr>
        </thead>

        <tbody>
            <% if (arrayMascotas.length > 0) { %>
                <% arrayMascotas.forEach(mascota => { %>
                    <tr>
                        <th scope="row"><%= mascota.id %> </th>
                        <td><%= mascota.name %> </td>
                        <td><%= mascota.description %> </td>
                        <td>@mdo</td>
                      </tr>
                <% }) %>
        <% } %>
        </tbody>
      </table>
   </div>

<%- include("template/footer")%>
